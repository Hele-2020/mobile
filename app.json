{
  "expo": {
      "packagerOpts": {
        "config": "metro.config.js",
        "sourceExts": ["js", "jsx", "svg"],
        "transformer": "node_modules/react-native-svg-transformer/index.js"
      },
    "name": "Projet Hélé",
    "slug": "projet-hele-mobile",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}